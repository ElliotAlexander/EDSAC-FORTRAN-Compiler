1014  FORMAT(14H AQUEOUS PHASE//
     X3(2A4,2(//13H X-COORDINATE,
     X2X,5(7H ISET =,1X,I2,6X)/
     X100(1X,I3,1X,1PE9.2,5(1X,1PE15.8)/)//)//)
     X9H  LIMAQ :,1X,I4)
