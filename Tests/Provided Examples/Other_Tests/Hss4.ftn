      SUBROUTINE SETC0

      COMMON/CB01/DCSL(10),BETA(10),SNA(10),C0(10),X0ALO(10), 

     XX0SIO(10),D(10),VB(10),CM0(10),SND(10)

      COMMON/CB05/NSET,ISET,NSTEP,ISTEP 

C      ROUTINE TO SET BULK IONIC STRENGTH 

      C0(9)=6.0E22

      C0(10)=2.0E22 

      DO 100 I=1,NSET 

      C0(I)=6.023E25

100   CONTINUE

      C0(1)=6.0E26

      C0(2)=2.0E26

      C0(3)=6.0E25

      C0(4)=2.0E25

      C0(5)=6.0E24

      C0(6)=2.0E24

      C0(7)=6.0E23

      C0(8)=2.0E23

      RETURN

      END 

