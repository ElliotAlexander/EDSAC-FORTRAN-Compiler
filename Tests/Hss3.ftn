      SUBROUTINE SETNA

      COMMON/CB01/DCSL(10),BETA(10),SNA(10),C0(10),X0ALO(10), 

     XX0SIO(10),D(10),VB(10),CM0(10),SND(10)

      COMMON/CB05/NSET,ISET,NSTEP,ISTEP 

C      ROUTINE TO SET SUBSTRATE ACCEPTOR LEVEL

      DO 100 I=1,NSET 

      SNA(I)=1E22 

100   CONTINUE

      RETURN

      END 

